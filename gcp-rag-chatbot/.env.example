# Copy this to .env and set your values
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# BigQuery dataset & table for vectors
BQ_DATASET=rag_demo
BQ_TABLE=docs
# Optional: explicit index name (defaults to idx_${BQ_TABLE}_emb)
BQ_VECTOR_INDEX=

# CSV ingestion settings
CSV_PATH=/absolute/path/to/your.csv
CSV_TEXT_COLUMN=text
CSV_ID_COLUMN=

# Gemini / RAG settings
TOP_K=5
EMBEDDING_MODEL=text-embedding-004
GENERATION_MODEL=gemini-1.5-pro-002

# Server
HOST=0.0.0.0
PORT=8080